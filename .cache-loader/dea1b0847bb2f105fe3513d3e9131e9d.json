() => {\n        const result = (await this.form.handleSubmit()).data;\n        if (result.success) {\n            this.props.onOk();\n        }\n    }\n\n    handleOk = () => {\n        this.handleSubmit();\n    }\n\n    handleCancel = () => {\n        this.props.onCancel();\n    }\n\n    render() {\n        const { isModalVisible: visible } = this.props;\n        return (\n            <Modal\n                className=\"d-pic-lib-modal\"\n                visible={visible}\n                title=\"新建页面\"\n                width={500}\n                onOk={this.handleOk}\n                onCancel={this.handleCancel}\n            >\n                <PageFormWrap wrappedComponentRef={(form: any) => {\n                    this.form = form;\n                }} />\n            </Modal>\n        );\n    }\n}\n\nexport default CreateModalModal;\n"]}]}